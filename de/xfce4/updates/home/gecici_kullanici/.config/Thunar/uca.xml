	<name>Uçbirimi Buraya Aç</name>
	<unique-id>1502567563531824-6</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Özel işlem için örnek</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
