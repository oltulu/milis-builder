	<name>Yetkili uçbirimi aç</name>
	<unique-id>1502567415246345-4</unique-id>
	<command>gksu &quot;xfce4-terminal --default-working-directory=%f&quot;</command>
	<description>Bu dizinde root terminali açmak için bu düğmeyi kullanın</description>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
	<text-files/>
